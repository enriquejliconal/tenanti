{
    "name": "orchestra/tenanti",
    "description": "Tenant based Database Schema Manager for Laravel",
    "keywords": ["laravel", "orchestral", "orchestra-platform", "multi-tenant", "schema", "migration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Tenanti\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tenanti\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Kernel.php"
        ]
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/console": "^9.0",
        "illuminate/filesystem": "^9.0",
        "orchestra/model": "^7.0",
        "orchestra/support": "^7.0"
    },
    "require-dev": {
        "laravel/tinker": "dev-develop",
        "orchestra/canvas": "^7.0",
        "orchestra/testbench": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
